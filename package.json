{
  "name": "@elasticlive/sdk",
  "version": "0.0.0-development",
  "description": "elasticlive",
  "main": "dist/ELive.min.js",
  "module": "dist/ELive.min.mjs",
  "unpkg": "dist/ELive.min.js",
  "jsdelivr": "dist/ELive.min.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run build:dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "npm run clean:dist && npm run clean:docs",
    "clean:dist": "rm -rf ./dist",
    "clean:docs": "rm -rf ./docs",
    "precommit": "pretty-quick --staged",
    "prettier": "prettier ./src/*",
    "build:dev": "rollup -c rollup.config.dev.js --watch",
    "build:production": "rollup -c rollup.config.prod.js",
    "build:docs": "jsdoc -c .jsdoc.json",
    "build": "npm run clean && npm run build:production && npm run build:docs",
    "deploy:npm": "cd release && npm publish --access public",
    "deploy:gh-pages": "cd release && npm run deploy",
    "deploy": "npm run deploy:npm && npm run deploy:gh-pages",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "WebRTC",
    "live",
    "broadcast",
    "broadcasting",
    "sdk",
    "p2p",
    "elasticlive",
    "remotemonster"
  ],
  "homepage": "https://elasticlive.io",
  "author": {
    "name": "RemoteMonster"
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/npm": "^5.1.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "husky": "^1.3.1",
    "jsdoc": "^3.5.5",
    "prettier": "^1.16.1",
    "pretty-quick": "^1.10.0",
    "rollup": "^1.4.1",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "semantic-release": "^15.13.3"
  },
  "dependencies": {
    "deepmerge": "^3.2.0",
    "events": "^3.0.0",
    "ipinfo": "^1.3.9",
    "platform": "^1.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elasticlive/web-sdk.git"
  },
  "publishConfig": {
    "access": "public",
    "publishConfig": {
      "tag": "next"
    }
  }
}
