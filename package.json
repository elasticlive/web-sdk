{
  "name": "@elasticlive/sdk",
  "version": "3.3.3",
  "description": "elasticlive",
  "main": "dist/ELive.min.js",
  "module": "dist/ELive.min.mjs",
  "unpkg": "dist/ELive.min.js",
  "jsdelivr": "dist/ELive.min.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run build:dev",
    "test": "npm-run-all test:unit test:integration:*",
    "test:unit": "mocha ./test/unit/index.js --require isomorphic-fetch --require babel-register --reporter spec --require babel-polyfill || exit 0",
    "test:e2e": "cypress run",
    "test:e2e:record": "cypress run --record",
    "test:e2e:browser": "cypress run --browser chrome",
    "test:integration:simplecall": "mocha ./test/integration/simplecall.js --require isomorphic-fetch --require babel-register --reporter spec --require babel-polyfill",
    "test:integration:simplecast": "mocha ./test/integration/simplecast.js --require isomorphic-fetch --require babel-register --reporter json --require babel-polyfill",
    "clean": "npm run clean:dist && npm run clean:docs",
    "clean:dist": "rm -rf ./dist",
    "clean:docs": "rm -rf ./docs",
    "precommit": "pretty-quick --staged",
    "prettier": "prettier ./src/*",
    "build:dev": "rollup -c rollup.config.dev.js --watch",
    "build:production": "npm run clean:dist && rollup -c rollup.config.prod.js",
    "build:docs": "npm run clean:docs && jsdoc -c .jsdoc.json",
    "build": "npm run build:production && npm run build:docs",
    "semantic-release": "semantic-release --dry-run"
  },
  "keywords": [
    "WebRTC",
    "CPaaS",
    "PaaS",
    "live",
    "broadcast",
    "broadcasting",
    "SDK",
    "P2P",
    "elasticlive",
    "RemoteMonster"
  ],
  "homepage": "https://elasticlive.io",
  "author": {
    "name": "RemoteMonster"
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/npm": "^5.1.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/selenium-webdriver": "^3.0.15",
    "babel-polyfill": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chromedriver": "^2.46.0",
    "cypress": "^3.2.0",
    "husky": "^1.3.1",
    "isomorphic-fetch": "^2.2.1",
    "jsdoc": "^3.5.5",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-spec-reporter": "0.0.32",
    "mocha": "^6.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.1",
    "pretty-quick": "^1.10.0",
    "rollup": "^1.6.0",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "semantic-release": "^15.13.3",
    "sinon": "^7.2.7"
  },
  "dependencies": {
    "deepmerge": "^3.2.0",
    "events": "^3.0.0",
    "ipinfo": "^1.3.9",
    "node-fetch": "^2.3.0",
    "platform": "^1.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elasticlive/web-sdk.git"
  },
  "publishConfig": {
    "access": "public",
    "publishConfig": {
      "tag": "next"
    }
  }
}
